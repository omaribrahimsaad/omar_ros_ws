		<!-- imu mag bias z -->
		<param name="imu_mag_bias_z" value="10"/>
		<!-- imu mag scale x -->
		<param name="imu_mag_scale_x" value="11"/>
		<!-- imu mag scale y -->
		<param name="imu_mag_scale_y" value="11"/>
		<!-- imu mag scale z -->
		<param name="imu_mag_scale_z" value="11"/>

		<!-- advance data request for monitor only -->
