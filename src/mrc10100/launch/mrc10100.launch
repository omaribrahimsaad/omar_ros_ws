<?xml version="1.0"?>
<launch>
	<!-- USB port name -->
	<arg name="port" default="/adev/ttyACM0"/>
	<!-- serial baud rate -->
	<arg name="baud_rate" default="115200"/>

	<!-- base link frame -->
	<arg name="base_frame_id" default="base_link"/>

	<!-- PID constant (P) for linear X  -->
	<arg name="pid_x_p" default="120"/>
	<!-- PID constant (I) for linear X  -->
	<arg name="pid_x_i" default="1600"/>
	<!-- PID constant (D) for linear X  -->
	<arg name="pid_x_d" default="6"/>
	<!-- PID coefficient derivative filter (N) for linear X  -->
	<arg name="pid_x_n" default="50"/>
	<!-- PID high limit output for linear X  -->
	<arg name="pid_x_high" default="1000"/>
	<!-- PID low limit output for linear X  -->
	<arg name="pid_x_low" default="-1000"/>
	<!-- PID integration windup for linear X  -->
	<arg name="pid_x_windup" default="true"/>

	<!-- PID constant (P) for angular Z  -->
	<arg name="pid_z_p" default="30"/>
	<!-- PID constant (I) for angular Z  -->
	<arg name="pid_z_i" default="461.5"/>
	<!-- PID constant (D) for angular Z  -->
	<arg name="pid_z_d" default="1.3"/>
	<!-- PID coefficient derivative filter (N) for angular Z  -->
	<arg name="pid_z_n" default="50"/>
	<!-- PID high limit output for angular Z  -->
	<arg name="pid_z_high" default="1000"/>
	<!-- PID low limit output for angular Z  -->
	<arg name="pid_z_low" default="-1000"/>
	<!-- PID integration windup for angular Z  -->
	<arg name="pid_z_windup" default="true"/>

	<!-- distance between wheel (differentational robot) -->
	<arg name="wheel_distance" default="0.468"/>
	<!-- right wheel motor diameter in meter (differentational robot) -->
	<arg name="wheel_right_size" default="0.15"/>
	<!-- left wheel motor diameter in meter (differentational robot) -->
	<arg name="wheel_left_size" default="0.15"/>

	<!-- count per rotation(CPR) of right wheel encoder -->
	<arg name="encoder_wheel_right_cpr" default="3600"/>
	<!-- number of coefficient(N) derivative filter right wheel encoder -->
	<arg name="encoder_wheel_right_n" default="50"/>
	<!-- count per rotation(CPR) of left wheel encoder -->
	<arg name="encoder_wheel_left_cpr" default="3600"/>
	<!-- number of coefficient(N) derivative filter left wheel encoder -->
	<arg name="encoder_wheel_left_n" default="50"/>

	<!-- imu acceleration bias x -->
	<arg name="imu_acc_bias_x" default="0"/>
	<!-- imu acceleration bias y -->
	<arg name="imu_acc_bias_y" default="0"/>
	<!-- imu acceleration bias z -->
	<arg name="imu_acc_bias_z" default="0"/>
	<!-- imu acceleration scale x -->
	<arg name="imu_acc_scale_x" default="1"/>
	<!-- imu acceleration scale y -->
	<arg name="imu_acc_scale_y" default="1"/>
	<!-- imu acceleration scale z -->
	<arg name="imu_acc_scale_z" default="1"/>

	<!-- imu gyro bias x -->
	<arg name="imu_gyro_bias_x" default="0"/>
	<!-- imu gyro bias y -->
	<arg name="imu_gyro_bias_y" default="0"/>
	<!-- imu gyro bias z -->
	<arg name="imu_gyro_bias_z" default="0"/>

	<!-- imu mag bias x -->
	<arg name="imu_mag_bias_x" default="0"/>
	<!-- imu mag bias y -->
	<arg name="imu_mag_bias_y" default="0"/>
	<!-- imu mag bias z -->
	<arg name="imu_mag_bias_z" default="0"/>
	<!-- imu mag scale x -->
	<arg name="imu_mag_scale_x" default="1"/>
	<!-- imu mag scale y -->
	<arg name="imu_mag_scale_y" default="1"/>
	<!-- imu mag scale z -->
	<arg name="imu_mag_scale_z" default="1"/>

	<!-- advance data request for monitor only -->
	<arg name="advance" default="true"/>
	<!-- debug data receive and send for monitor only -->
	<arg name="debug" default="true"/>

